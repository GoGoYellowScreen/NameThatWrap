@using NameThatWrap.Models;
@using NameThatWrap.data;

@model SignInModel

@{
    ViewBag.Title = "SignIn";
    Layout = "../Shared/_Layout.cshtml";
}


<body>
    <div class="form">
        <h3>Sign in:</h3>
        @using (Html.BeginForm("SignIn", "Account"))
        {
            @Html.ValidationSummary();
            <label>Email:</label> @Html.TextBoxFor(m => m.Email)<br/>
            <label>Password:</label> @Html.PasswordFor(m => m.Password)<br/>
            <input type="submit" value="Sign in!" />

        }
    </div>
</body>