@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <title>Name That Wrap</title>
    <meta name="description" content="How well do you know woven wraps? Test your ability to identify the colorways of popular brands with quiz.">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="../../css/main.css">
</head>
<body>
    <div class="form">
        <h1 id="title">Name <span>that</span> Wrap</h1>
        <div id="log-in">
            <p><a class="fauxbutton" href="@Url.Action("Register", "Account")">Register</a></p>
            <p><a class="fauxbutton" href="@Url.Action("SignIn", "Account")">Sign In</a></p>
            <p><a class="fauxbutton" href="@Url.Action("Suggestion", "Feedback")">Suggest a wrap to add</a></p>
        </div><!--end log-in div-->
    </div><!--end form div-->
</body>
</html>
